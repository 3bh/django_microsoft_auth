{
    "version": "2",
    "excluded_patterns": [
        "versioneer.py",
        "microsoft_auth/_version.py",
        "config",
        "db",
        "dist",
        "features",
        "**/node_modules",
        "script",
        "**/spec",
        "**/test",
        "**/tests",
        "**/vendor",
        "**/*.d.ts"
    ],
    "plugins": {
        "csslint": {
            "enabled": true
        },
        "duplication": {
            "enabled": true
       },
       "fixme": {
            "enabled": true
       },
       "pep8": {
            "enabled": true
       },
       "radon": {
            "enabled": true
       }
    }
}
